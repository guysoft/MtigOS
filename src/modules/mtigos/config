
# Docker MTIG repo
[ -n "$MTIGOS_DOCKER_MTIG_REPO_SHIP" ] || MTIGOS_DOCKER_MTIG_REPO_SHIP=https://github.com/guysoft/docker-mtig.git
[ -n "$MTIGOS_DOCKER_MTIG_REPO_BUILD" ] || MTIGOS_DOCKER_MTIG_REPO_BUILD=
[ -n "$MTIGOS_DOCKER_MTIG_REPO_BRANCH" ] || MTIGOS_DOCKER_MTIG_REPO_BRANCH=master
[ -n "$MTIGOS_DOCKER_MTIG_REPO_DEPTH" ] || MTIGOS_DOCKER_MTIG_REPO_DEPTH=1

